<easyfk-application name="catalina"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://www.gorun8.cn/dtds/easyfk-application.xsd">
    <resource-loader name="main" type="component"/>
    <classpath type="jar" location="lib/*"/>
    <classpath type="jar" location="build/lib/*"/>
    <classpath type="dir" location="config"/>
    <entity-resource type="model" reader-name="main" loader="main" location="entitydef/${db.type}/catalina.sql"/>
    
</easyfk-application>
